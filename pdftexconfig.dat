% tex-ini-files 2016-02-27: pdftexconfig.dat

% Shared configuration for pdfTeX/LuaTeX low-level settings

% Paper size: dimensions given in absolute terms
pageheight       = 297 true mm
pagewidth        = 210 true mm

% Enable PDF 1.5 output and thus more compression
minorversion     = 5
objcompresslevel = 2

% Low-level settings unlikely ever to need to change
compresslevel    = 9
decimaldigits    = 3
pkresolution     = 600
horigin          = 1 true in
vorigin          = 1 true in
